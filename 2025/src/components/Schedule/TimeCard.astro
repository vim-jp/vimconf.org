---
import Card from "../Card.astro";
import { Clock, User } from "@lucide/astro";
---

<Card>
<div class="p-4">
  <div class="space-y-3">
    <div class="flex items-center">
      <Clock class="h-4 w-4 mr-2 flex-shrink-0" />
      <span class="text-sm font-bold text-gray-800">9:30 - 10:00</span>
    </div><div class="flex items-start">
      <div class="mt-1 mr-3 flex-shrink-0 text-gray-600">
        <User class="h-3 w-3 mr-1 text-gray-500 flex-shrink-0" />
      </div><div class="min-w-0 flex-1">
        <h3 class="mb-2 text-base leading-tight font-semibold text-gray-900">
          受付・開場
        </h3>
      </div>
    </div>
  </div>
</div>

</Card>
